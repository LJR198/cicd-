# 应用配置
NODE_ENV=production
PORT=80

# API 配置
API_URL=https://api.example.com
API_TIMEOUT=30000

# 时区
TZ=Asia/Shanghai

# Docker Hub 配置
DOCKER_USERNAME=your-dockerhub-username
